enum Permission {
  SUPERADMIN
  ADMIN
  USER
  ARTIST
  PERMISSIONUPDATE
}

type User {
  id: ID! @id
  email: String! @unique
  password: String!
  createdAt: DateTime!
  updatedAt: DateTime!
  emailConfirmToken: String!
  emailConfirmed: Boolean
  permissions: [Permission]! @scalarList(strategy: RELATION)
}
